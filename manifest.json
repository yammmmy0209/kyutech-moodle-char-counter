{
    "manifest_version": 3,
    "name": "九工大Moodle 文字数カウンター",
    "version": "1.0",
    "description": "九工大Moodleのテキストエディタに、正しい文字数をカウントするカウンターを表示します。",
    "content_scripts": [
        {
            "matches": ["https://*.el.kyutech.ac.jp/*"],
            "js": ["content.js"]
        }
    ],
    "icons": {
        "48": "icon48.png"
    }
}
